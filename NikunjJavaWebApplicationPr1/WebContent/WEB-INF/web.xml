         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	<servlet>
		<servlet-name>secondTest</servlet-name>
		<servlet-class>com.servlet.SecondServlet</servlet-class>
	</servlet>
	<servlet-mapping>
